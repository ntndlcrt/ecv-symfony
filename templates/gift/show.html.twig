{% extends 'base.html.twig' %}

{% block title %}Gift{% endblock %}

{% block body %}
    <h1>Gift</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ gift.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ gift.title }}</td>
            </tr>
            <tr>
                <th>Src</th>
                <td>{{ gift.src }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ gift.price }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ gift.url }}</td>
            </tr>
            <tr>
                <th>Excerpt</th>
                <td>{{ gift.excerpt }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_gift_index') }}">back to list</a>

    <a href="{{ path('app_gift_edit', {'id': gift.id}) }}">edit</a>

    {{ include('gift/_delete_form.html.twig') }}
{% endblock %}
